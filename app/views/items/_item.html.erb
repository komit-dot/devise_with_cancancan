<div id="<%= dom_id item %>">
  <p>
    <strong>Name:</strong>
    <%= item.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= item.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= item.price %>
  </p>

  <p>
    <strong>User:</strong>
    <%= item.user %>
  </p>

  <p>
    <strong>Belongs to:</strong>
    <%= item.belongs_to %>
  </p>

</div>
